[alias]
  co = checkout
  st = status
  ci = commit
  pom = push origin master
  br = branch
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
  rom = rebase origin/master master

[branch "master"]
  remote = origin
  merge = refs/heads/master

[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true

[color "status"]
  added = yellow
  changed = green
  untracked = cyan

[push]
	default = simple
